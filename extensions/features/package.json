		"package": "webpack --mode production --devtool hidden-source-map"
	},
	"devDependencies": {
		"@types/node": "14.x",
		"@types/vscode": "^1.30.0",
		"@xcheme/core": "^0.0.9",
		"@xcheme/lang": "^0.0.10",
		"@xcheme/lexer": "^0.0.1",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	}
}
